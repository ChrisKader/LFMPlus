<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">
    <Frame name="LFMPlusFrame" parent="GroupFinderFrame" inherits="BackdropTemplate">
        <Size x="250" y="50"/>
        <KeyValues>
            <KeyValue key="backdropInfo" value="TOOLTIP_BACKDROP_STYLE_DEFAULT" type="global"/>
            <KeyValue key="backdropColor" value="TOOLTIP_DEFAULT_BACKGROUND_COLOR" type="global"/>
            <KeyValue key="backdropBorderColor" value="TOOLTIP_DEFAULT_COLOR" type="global"/>
        </KeyValues>
        <Anchors>
            <Anchor point="BOTTOMRIGHT" relativePoint="TOPRIGHT"/>
        </Anchors>
        <Frames>
            <Frame hidden="true" name="$parent_results">
            </Frame>
            <Slider name="$parent_ScoreMin" inherits="OptionsSliderTemplate" valueStep="100" obeyStepOnDrag="true">
                <Size x="126" y="17"/>
                <Anchors>
                    <Anchor point="RIGHT" relativeTo="$parent" relativePoint="RIGHT">
                    <Offset x="-10"/>
                    </Anchor>
                </Anchors>
            </Slider>
        </Frames>
    </Frame>
</Ui>
